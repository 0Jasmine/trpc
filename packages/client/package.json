{
  "name": "@trpc/client",
  "version": "10.45.0",
  "description": "The tRPC client library",
  "author": "KATT",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/client"
  },
  "eslintConfig": {
    "rules": {
      "no-restricted-imports": [
        "error",
        "@trpc/client"
      ]
    }
  },
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "dev": "pnpm build --watch",
    "codegen-entrypoints": "tsx entrypoints.script.ts",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --ignore-path ../../.gitignore src",
    "ts-watch": "tsc --watch"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./links/httpBatchLink": {
      "import": {
        "types": "./dist/links/httpBatchLink.d.ts",
        "default": "./dist/links/httpBatchLink.mjs"
      },
      "require": {
        "types": "./dist/links/httpBatchLink.d.ts",
        "default": "./dist/links/httpBatchLink.js"
      }
    },
    "./links/httpLink": {
      "import": {
        "types": "./dist/links/httpLink.d.ts",
        "default": "./dist/links/httpLink.mjs"
      },
      "require": {
        "types": "./dist/links/httpLink.d.ts",
        "default": "./dist/links/httpLink.js"
      }
    },
    "./links/loggerLink": {
      "import": {
        "types": "./dist/links/loggerLink.d.ts",
        "default": "./dist/links/loggerLink.mjs"
      },
      "require": {
        "types": "./dist/links/loggerLink.d.ts",
        "default": "./dist/links/loggerLink.js"
      }
    },
    "./links/splitLink": {
      "import": {
        "types": "./dist/links/splitLink.d.ts",
        "default": "./dist/links/splitLink.mjs"
      },
      "require": {
        "types": "./dist/links/splitLink.d.ts",
        "default": "./dist/links/splitLink.js"
      }
    },
    "./links/wsLink": {
      "import": {
        "types": "./dist/links/wsLink.d.ts",
        "default": "./dist/links/wsLink.mjs"
      },
      "require": {
        "types": "./dist/links/wsLink.d.ts",
        "default": "./dist/links/wsLink.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "links",
    "!**/*.test.*"
  ],
  "dependencies": {
    "@trpc/core": "10.45.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.0.0",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/node": "^20.10.0",
    "eslint": "^8.40.0",
    "isomorphic-fetch": "^3.0.0",
    "node-fetch": "^3.3.0",
    "rollup": "^2.79.1",
    "tsx": "^4.0.0",
    "undici": "^6.0.1",
    "vitest": "^0.32.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": [
    "https://trpc.io/sponsor"
  ]
}
